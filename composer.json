{
    "name": "dansoppelsa/laravel-carbon-macros",
    "description": "Laravel Carbon Macros",
    "type": "library",
    "license": "MIT",
    "require": {
        "illuminate/support": ">=5.5"
    },
    "require-dev": {
        "orchestra/testbench": "^4.5"
    },
    "authors": [
        {
            "name": "Dan Soppelsa",
            "email": "dsoppelsa@gmail.com"
        }
    ],
    "autoload": {
        "psr-4" : {
            "CarbonMacros\\" : "src"
        }
    },
    "autoload-dev" : {
        "psr-4" : {
            "CarbonMacros\\" : "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "CarbonMacros\\CarbonMacrosServiceProvider"
            ]
        }
    }
}
