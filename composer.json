{
    "name": "dansoppelsa/laravel-carbon-macros",
    "description": "Laravel Carbon Macros",
    "type": "library",
    "license": "MIT",
    "require": {
        "illuminate/support": ">=5.5"
    },
    "require-dev": {
        "orchestra/testbench": "^7.0|^8.0|^9.0|^10.0",
        "phpunit/phpunit": "^10.0|^11.0"
    },
    "authors": [
        {
            "name": "Dan Soppelsa",
            "email": "dsoppelsa@gmail.com"
        }
    ],
    "autoload": {
        "psr-4" : {
            "CarbonMacros\\" : "src"
        }
    },
    "autoload-dev" : {
        "psr-4" : {
            "CarbonMacros\\" : "tests",
            "CarbonMacros\\Dev\\" : "dev"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "CarbonMacros\\CarbonMacrosServiceProvider"
            ]
        }
    }
}
